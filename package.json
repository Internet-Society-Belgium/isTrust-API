{
  "name": "trest-api",
  "version": "0.1.0",
  "description": "trest API",
  "homepage": "https://github.com/Progitiel/trest-api#readme",
  "bugs": {
    "url": "https://github.com/Progitiel/trest-api/issues"
  },
  "author": {
    "name": "Progiciel",
    "url": "https://progiciel.be/"
  },
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Progitiel/trest-api.git"
  },
  "scripts": {
    "start": "node .",
    "dev": "node -r dotenv/config .",
    "build": "node utils/prebuild.js && tsc && node utils/postbuild.js",
    "dev:watch": "nodemon --exec 'npm run build && npm run dev' --ignore build/",
    "lint": "eslint --fix ./src",
    "format": "prettier -w -u ./src"
  },
  "dependencies": {
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "helmet": "^4.6.0",
    "maxmind": "^4.3.2",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/tar": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
